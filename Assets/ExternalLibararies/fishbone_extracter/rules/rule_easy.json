{
    "rules": [
        {
            "name": "_ps",
            "rule": {
                "type": "c",
                "c": "_ps"
            }
        },
        {
            "name": "_psら",
            "rule": {
                "type": "c",
                "c": "_psら"
            }
        },
        {
            "name": "_ps+名詞",
            "rule": {
                "type": "regex",
                "c": "_ps.+"
            }
        },
        {
            "name": "英字+英数字+名詞",
            "rule": {
                "type": "regex",
                "c": "[A-Z]+[0-9A-Z]*.+"
            }
        },
        {
            "name": "名詞+英数字",
            "rule": {
                "type": "regex",
                "c": "[^0-9A-Z]+[A-Z]+[0-9A-Z]*"
            }
        },
        {
            "name": "英数字",
            "rule": {
                "type": "regex",
                "c": "[A-Z]+[0-9A-Z]*"
            }
        }
    ]
}